<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="510.0" prefWidth="618.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.lineChart.LineChartController">
   <children>
      <LineChart fx:id="lineChart" layoutY="78.0" title="LINE CHART">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Label layoutX="134.0" layoutY="14.0" prefHeight="64.0" prefWidth="303.0" text="LineChart">
         <font>
            <Font name="Algerian" size="56.0" />
         </font>
      </Label>
      <Button layoutX="499.0" layoutY="100.0" mnemonicParsing="false" onAction="#onControlHeater" prefHeight="46.0" prefWidth="108.0" text="Control Heater">
         <font>
            <Font name="Century Gothic Bold" size="11.0" />
         </font>
      </Button>
      <Button layoutX="540.0" layoutY="189.0" mnemonicParsing="false" onAction="#onBackButton" text="Back">
         <font>
            <Font name="Century Gothic" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
