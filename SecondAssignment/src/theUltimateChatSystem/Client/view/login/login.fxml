<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="298.0" prefWidth="355.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="theUltimateChatSystem.Client.view.login.LoginController">
   <children>
      <TextField fx:id="username" layoutX="88.0" layoutY="73.0" />
      <TextField fx:id="password" layoutX="88.0" layoutY="138.0" />
      <Label layoutX="24.0" layoutY="77.0" text="Username" />
      <Label layoutX="25.0" layoutY="142.0" text="Password" />
      <Label layoutX="145.0" layoutY="21.0" text="LOGIN" />
      <Button layoutX="228.0" layoutY="249.0" mnemonicParsing="false" onAction="#onLoginAsGuest" text="Login as a guest" />
      <Button layoutX="266.0" layoutY="112.0" mnemonicParsing="false" onAction="#onLogin" text="Login" />
      <Button layoutX="59.0" layoutY="249.0" mnemonicParsing="false" onAction="#onCreateAccount" text="Create an account" />
      <Label layoutX="50.0" layoutY="209.0" text="Dont have an account ??" />
      <Label fx:id="errorLabel" layoutX="45.0" layoutY="180.0" prefHeight="18.0" prefWidth="198.0" text="Error............................................................" textFill="RED" />
   </children>
</AnchorPane>
