<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="theUltimateChatSystem.Client.view.createAccount.CreateAccountController">
   <children>
      <TextField fx:id="username" layoutX="135.0" layoutY="127.0" />
      <TextField fx:id="password" layoutX="135.0" layoutY="174.0" />
      <TextField fx:id="confirm" layoutX="135.0" layoutY="229.0" />
      <Label layoutX="61.0" layoutY="131.0" text="Username" />
      <Label layoutX="62.0" layoutY="178.0" text="Password" />
      <Label layoutX="70.0" layoutY="217.0" text="Confirm" />
      <Label layoutX="164.0" layoutY="24.0" prefHeight="44.0" prefWidth="171.0" text="Sign UP">
         <font>
            <Font name="Elephant" size="41.0" />
         </font>
      </Label>
      <Label layoutX="66.0" layoutY="242.0" text="Password" />
      <Label fx:id="errorLabel" layoutX="34.0" layoutY="298.0" prefHeight="18.0" prefWidth="332.0" text="Error................................................................................................................" textFill="RED" />
      <Button layoutX="326.0" layoutY="127.0" mnemonicParsing="false" onAction="#onSignUp" text="Sign UP" />
      <Button layoutX="326.0" layoutY="183.0" mnemonicParsing="false" onAction="#onLoginAsGuest" text="Login as a guest" />
      <Button layoutX="158.0" layoutY="334.0" mnemonicParsing="false" onAction="#onAlreadyAccount" text="I already have an account" />
   </children>
</AnchorPane>
