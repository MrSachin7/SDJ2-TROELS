<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="323.0" prefWidth="323.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="theUltimateChatSystem.Client.view.loginAsGuest.UserNameController">
   <children>
      <Label layoutX="82.0" layoutY="87.0" prefHeight="33.0" prefWidth="158.0" text="Enter a username">
         <font>
            <Font name="Berlin Sans FB" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="63.0" layoutY="148.0" prefHeight="26.0" prefWidth="189.0" promptText="what other clients will see you as" />
      <Label fx:id="statusLabel" layoutX="89.0" layoutY="26.0" prefHeight="18.0" prefWidth="208.0" text="Status with the server" textFill="RED" />
      <Button layoutX="126.0" layoutY="207.0" mnemonicParsing="false" onAction="#onConnect" text="Connect ">
         <font>
            <Font name="Bookman Old Style" size="12.0" />
         </font>
      </Button>
      <Label fx:id="serverMessage" layoutX="14.0" layoutY="285.0" text="Any messages like if username already occupied" textFill="RED" />
   </children>
</AnchorPane>
